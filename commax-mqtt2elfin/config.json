{
  "name": "MQTT2ELFIN for SEIK Controller",
  "version": "1.2.4",
  "slug": "commax_mqtt2elfin_python",
  "url": "https://github.com/tirasol/HAaddon",
  "description": "MQTT + ELFIN SOCKET 통신을 활용한 세익(SEIK) 컨트롤러 애드온",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "map": [
    "share:rw"
  ],
  "startup": "application",
  "boot": "auto",
  "options": {
    "DEBUG": true,
    "mqtt_log": true,
    "elfin_log": true,
    "save_unregistered_signal": true,
    "mqtt_server": "192.168.88.xx",
    "mqtt_id": "mqtt",
    "mqtt_password": "!SUPERSTRONGPASSWORD",
    "elfin_server": "192.168.88.xx",
    "elfin_id": "admin",
    "elfin_password": "admin"
  },
  "schema": {
    "DEBUG": "bool",
    "mqtt_log": "bool",
    "elfin_log": "bool",
    "save_unregistered_signal": "bool",
    "mqtt_server": "str",
    "mqtt_id": "str",
    "mqtt_password": "str",
    "elfin_server": "str",
    "elfin_id": "str",
    "elfin_password": "str",
    "Number_of_households": "str",
    "1_ip": "str",
    "1_lake": "str",
    "1_valves": "str",
    "2_ip": "str?",
    "2_lake": "str?",
    "2_valves": "str?",
    "3_ip": "str?",
    "3_lake": "str?",
    "3_valves": "str?",
    "4_ip": "str?",
    "4_lake": "str?",
    "4_valves": "str?",
    "5_ip": "str?",
    "5_lake": "str?",
    "5_valves": "str?"
  }
}
