{
  "name": "MQTT2elfin COMMAX Wallpad Controller with Python",
  "version": "1.2.1",
  "slug": "commax_mqtt2elfin_python",
  "url": "https://github.com/kimtc99/HAaddon",
  "description": "mqtt 통신을 활용한 코맥스 월패드 컨트롤러",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "map": [
    "share:rw"
  ],
  "startup": "application",
  "boot": "auto",
  "options": {
    "DEBUG": true,
    "mqtt_log": true,
    "elfin_log": true,
    "save_unregistered_signal": true,
    "mqtt_server": "192.168.88.11",
    "mqtt_id": "mqtt",
    "mqtt_password": "!SUPERSTRONGPASSWORD",
    "elfin_server": "192.168.88.12",
    "elfin_id": "admin",
    "elfin_password": "admin"
  },
  "schema": {
    "DEBUG": "bool",
    "mqtt_log": "bool",
    "elfin_log": "bool",
    "save_unregistered_signal": "bool",
    "mqtt_server": "str",
    "mqtt_id": "str",
    "mqtt_password": "str",
    "elfin_server": "str",
    "elfin_id": "str",
    "elfin_password": "str",
    "Number_of_households": "str",
    "1_ip": "str",
    "1_lake": "str",
    "1_valves": "str",
    "2_ip": "str",
    "2_lake": "str",
    "2_valves": "str"
  }
}
