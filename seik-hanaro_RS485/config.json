{
  "name": "SEIK-HANARO RS485 Controller",
  "version": "1.4.4",
  "slug": "seik-hanaro_rs485",
  "url": "https://github.com/tabspacekr/HomeAssistant-Addon",
  "description": "MQTT + ELFIN SOCKET 통신을 활용한 세익(SEIK) 하나로(HANARO) 온도조절기 컨트롤러 애드온",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "map": [
    "share:rw"
  ],
  "startup": "application",
  "boot": "auto",
  "options": {
    "DEBUG": true,
    "mqtt_log": true,
    "elfin_log": true,
    "save_unregistered_signal": true,
    "mqtt_server": "192.168.88.11",
    "mqtt_id": "mqtt",
    "mqtt_password": "!SUPERSTRONGPASSWORD"
  },
  "schema": {
    "DEBUG": "bool",
    "mqtt_log": "bool",
    "elfin_log": "bool",
    "save_unregistered_signal": "bool",
    "mqtt_server": "str",
    "mqtt_id": "str",
    "mqtt_password": "str"
  }
}
